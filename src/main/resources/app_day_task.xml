<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<properties>
		<tasks.parallel.permits>2</tasks.parallel.permits>
		<tasks>
			<task>
				<name>搜索分析</name>
				<args><![CDATA[{"logTypes":"S","granularity":"DAY","configFile":"parameters_search_indicators.xml",
				"mapReduceClassName":"SearchIndicatorMapReduce"}]]></args>
			</task>
			<task>
				<name>搜索词分析</name>
				<args><![CDATA[{"logTypes":"S","granularity":"DAY","configFile":"parameters_searchword_indicators.xml",
				"mapReduceClassName":"SearchWordsMapReduce"}]]></args>
			</task>
			<task>
				<name>事件分析</name>
				<args><![CDATA[{"logTypes":"V","granularity":"DAY","configFile":"parameters_event_indicators.xml",
				"mapReduceClassName":"EventIndicatorMapReduce"}]]></args>
			</task>
			<task>
				<name>页面分析</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_page_indicators.xml",
				"mapReduceClassName":"ActivityPageIndicatorMapReduce"}]]></args>
			</task>
			<task>
				<name>路径分析</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_path_indicators.xml",
				"mapReduceClassName":"ActivityPathIndicatorMapReduce"}]]></args>
			</task>
			<task>
				<name>用户基础分析</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_indicators.xml",
				"mapReduceClassName":"ActivityUsersIndicatorMapReduce"}]]></args>
			</task>
			<task>
				<name>终端分析</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_indicators_terminal.xml",
				"mapReduceClassName":"ActivityMapReduce"}]]></args>
			</task>
			<task>
				<name>留存用户</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_users_retained.xml",
				"mapReduceClassName":"ActivityUsersRetainedMapRecude"}]]></args>
			</task>
			<task>
				<name>使用时长分布</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_usetime.xml",
				"mapReduceClassName":"ActivityUseTimeMapReduce1"}]]></args>
			</task>
			<task>
				<name>面访问深度</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_page_deep.xml",
				"mapReduceClassName":"ActivityPageDeepMapReduce1"}]]></args>
			</task>
			<task>
				<name>启动次数分布 </name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY","configFile":"parameters_activity_startDist.xml",
				"mapReduceClassName":"ActivityStartDistMapReduce1"}]]></args>
			</task>
			<task>
				<name>回访用户</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY7","configFile":"parameters_user_revisit_indicators.xml",
				"mapReduceClassName":"ActivityRevisitUsersMapReduce1"}]]></args>
			</task>
			<task>
				<name>启动间隔分布</name>
				<args><![CDATA[{"logTypes":"A","granularity":"DAY7","configFile":"parameters_user_startInterval.xml",
				"mapReduceClassName":"ActivityStartIntervalMapReduce1"}]]></args>
			</task>
			
		</tasks>
	</properties>
</configuration>